<!doctype html>
<html>
<head>
    <meta name="viewport" content="width = device-width, initial-scale = 1.0">
    <link href="https://fonts.googleapis.com/css?family=Raleway" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
    <link href="/static/css/adminlogin_stylesheet.css" rel="stylesheet" type="text/css">
</head>
<h1> Admin Login </h1>
<div class ="mainbody">
  <% if (message.length > 0) { %>
       <div><%= message %></div>
   <% } %>
  <form class="loginform" method="post" action = "/admin_login">
    <label >Username</label>
    <input class="username_input" type="text" name="loginId">
    <label>Password</label>
    <input type="password" class="password_input" name="password">
    <button type="submit" class="Login">Login</button>
  </form>
</div>
</html>
